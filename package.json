{
  "name": "husky-test",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "npm run tsc && npm run eslint && npm run stylelint"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "eslint": "^9.5.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "stylelint": "^16.6.1",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vue-tsc": "^2.0.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "node scripts/verify-commit-msg.js"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "npm run lint",
      "git add"
    ]
  }
}
